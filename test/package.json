{
  "name": "@zephyr3d/test",
  "version": "0.0.1",
  "description": "zephyr3d test cases",
  "homepage": "https://github.com/gavinyork/zephyr3d#readme",
  "private": "true",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavinyork/zephyr3d.git"
  },
  "scripts": {
    "copy-assets": "shx mkdir -p dist/js && shx cp -R src/assets dist && shx cp src/index.html dist",
    "clean": "shx rm -rf dist .tsbuildinfo",
    "tsc": "tsc -p . --noEmit",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 rollup -c --silent && npm run copy-assets",
    "dev-test": "web-dev-server --open dist/index.html"
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "devDependencies": {
    "@web/dev-server": "^0.1.35",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-strip": "^3.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "@rollup/pluginutils": "^5.0.2",
    "rollup": "^3.15.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-copy": "^3.4.0",
    "@swc/core": "^1.3.62",
    "rollup-plugin-swc3": "^0.8.2",
    "prettier": "^2.8.4",
    "replace": "^1.2.2",
    "run-script-os": "^1.1.6",
    "shx": "^0.3.4",
    "cross-env": "^7.0.3",
    "typescript": "^5.1.3",
    "tslib": "^2.5.0"
  },
  "dependencies": {
    "@types/colors": "^1.2.1",
    "@types/diff": "^5.0.2",
    "@webgpu/types": "^0.1.31",
    "colors": "^1.4.0",
    "diff": "^5.0.0",
    "lodash-es": "^4.17.21",
    "rxjs": "7.5.2",
    "@zip.js/zip.js": "^2.7.35",
    "ts-keycode-enum": "^1.0.6",
    "gl-matrix": "^3.4.3",
    "@zephyr3d/base": "workspace:^0.1.0",
    "@zephyr3d/device": "workspace:^0.1.0",
    "@zephyr3d/scene": "workspace:^0.1.0",
    "@zephyr3d/imgui": "workspace:^0.1.0",
    "@zephyr3d/backend-webgl": "workspace:^0.1.0",
    "@zephyr3d/backend-webgpu": "workspace:^0.1.0"
  }
}
