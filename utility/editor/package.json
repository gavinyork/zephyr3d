{
  "name": "@zephyr3d/editor",
  "version": "0.0.1",
  "description": "zephyr3d studio",
  "homepage": "https://github.com/gavinyork/zephyr3d#readme",
  "private": "true",
  "files": [
    "dist"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavinyork/zephyr3d.git"
  },
  "scripts": {
    "clean": "shx rm -rf dist .tsbuildinfo",
    "build:check": "tsc -p . --noEmit",
    "lint": "cross-env NODE_OPTIONS=--max_old_space_size=8192 eslint \"src/**/*.ts\" -f compact",
    "lint-fix": "cross-env NODE_OPTIONS=--max_old_space_size=8192 eslint --fix \"src/**/*.ts\" -f compact",
    "build": "npm run build:app && npm run build:lib",
    "build:app": "cross-env NODE_OPTIONS=--max_old_space_size=8192 vite build --mode production",
    "build:lib": "cross-env NODE_OPTIONS=--max_old_space_size=8192 rollup -c --silent",
    "serve": "web-dev-server --no-cache --open dist/index.html"
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "devDependencies": {

    "@web/dev-server": "^0.1.35",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-node-resolve": "~16.0.0",
    "@rollup/plugin-strip": "~3.0.4",
    "@rollup/plugin-terser": "~0.4.4",
    "@rollup/plugin-commonjs": "~28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "rollup-plugin-sourcemaps2": "^0.5.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-css-only": "^4.5.2",
    "@rollup/pluginutils": "~5.1.4",
    "rollup": "~4.30.1",
    "rollup-plugin-dts": "~6.1.1",
    "rollup-plugin-copy": "^3.4.0",
    "@swc/core": "^1.3.62",
    "rollup-plugin-swc3": "~0.12.1",
    "run-script-os": "^1.1.6",
    "shx": "^0.3.4",
    "cross-env": "^7.0.3",
    "typescript": "^5.7.3",
    "tslib": "^2.8.1",
    "vite": "^7.1.1",
    "terser": "^5.16.0",
    "vite-plugin-static-copy": "^3.1.1",
    "vite-plugin-monaco-editor-esm": "^2.0.2",
    "eslint": "~9.38.0",
    "@eslint/js": "~9.38.0",
    "@typescript-eslint/parser": "~8.46.1",
    "@typescript-eslint/eslint-plugin": "~8.46.1",
    "prettier": "~3.6.2",
    "eslint-plugin-prettier": "~5.5.4",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-import": "~2.32.0",
    "eslint-import-resolver-typescript": "~4.4.4",
    "eslint-plugin-tsdoc": "~0.4.0",
    "globals": "^16.4.0",
    "eslint-formatter-compact": "~8.40.0"
  },
  "dependencies": {
    "@types/node": "^22.12.0",
    "@types/upng-js": "^2.1.5",
    "@types/streamsaver": "^2.0.5",
    "@types/draco3d": "^1.4.9",
    "@webgpu/types": "^0.1.65",
    "idb": "^8.0.1",
    "@zip.js/zip.js": "^2.7.54",
    "upng-js": "^2.1.0",
    "streamsaver": "^2.0.6",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@zephyr3d/base": "workspace:^0.2.0",
    "@zephyr3d/device": "workspace:^0.2.5",
    "@zephyr3d/scene": "workspace:^0.7.0",
    "@zephyr3d/imgui": "workspace:^0.2.0",
    "@zephyr3d/backend-webgl": "workspace:^0.1.9",
    "@zephyr3d/backend-webgpu": "workspace:^0.1.8",
    "@rollup/browser": "^4.46.2",
    "es-module-lexer": "^1.7.0",
    "typescript": "^5.7.3",
    "monaco-editor": "^0.52.0",
    "@monaco-editor/loader": "^1.5.0",
    "rollup-plugin-import-css": "~3.5.8"
  }
}
