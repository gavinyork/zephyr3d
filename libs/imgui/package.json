{
  "name": "@zephyr3d/imgui",
  "version": "0.1.0",
  "description": "imgui binding for zephyr3d",
  "homepage": "https://github.com/gavinyork/zephyr3d#readme",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "**/*.js.map"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavinyork/zephyr3d.git"
  },
  "scripts": {
    "clean": "shx rm -rf ./dist .tsbuildinfo",
    "tsc": "tsc -p . --noEmit",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 rollup -c --silent"
  },
  "keywords": [
    "javascript",
    "3d",
    "rendering",
    "engine",
    "webgl",
    "webgpu",
    "canvas",
    "zephyr3d",
    "device"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gavinyork/zephyr3d/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-strip": "^3.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/pluginutils": "^5.0.2",
    "rollup": "^3.15.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "@swc/core": "^1.3.62",
    "rollup-plugin-swc3": "^0.8.2",
    "shx": "^0.3.4",
    "cross-env": "^7.0.3",
    "typescript": "^5.1.3",
    "@zephyr3d/base": "workspace:^0.1.0",
    "@zephyr3d/device": "workspace:^0.1.0"
  },
  "peerDependencies": {
    "@zephyr3d/base": "workspace:^0.1.0",
    "@zephyr3d/device": "workspace:^0.1.0"
  },
  "dependencies": {
    "@webgpu/types": "^0.1.31",
    "@types/node": "^18.13.0",
    "@types/emscripten": "^1.39.6"
  }
}
