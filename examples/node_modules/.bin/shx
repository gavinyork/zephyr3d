#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/zephyr3d/zephyr3d/common/temp/node_modules/.pnpm/shx@0.3.4/node_modules/shx/lib/node_modules:/home/runner/work/zephyr3d/zephyr3d/common/temp/node_modules/.pnpm/shx@0.3.4/node_modules/shx/node_modules:/home/runner/work/zephyr3d/zephyr3d/common/temp/node_modules/.pnpm/shx@0.3.4/node_modules:/home/runner/work/zephyr3d/zephyr3d/common/temp/node_modules/.pnpm/node_modules:/home/runner/work/zephyr3d/zephyr3d/common/temp/node_modules:/home/runner/work/zephyr3d/zephyr3d/common/node_modules:/home/runner/work/zephyr3d/zephyr3d/node_modules:/home/runner/work/zephyr3d/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/zephyr3d/zephyr3d/examples/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/runner/work/zephyr3d/zephyr3d/common/temp/node_modules/.pnpm/shx@0.3.4/node_modules/shx/lib/node_modules:/home/runner/work/zephyr3d/zephyr3d/common/temp/node_modules/.pnpm/shx@0.3.4/node_modules/shx/node_modules:/home/runner/work/zephyr3d/zephyr3d/common/temp/node_modules/.pnpm/shx@0.3.4/node_modules:/home/runner/work/zephyr3d/zephyr3d/common/temp/node_modules/.pnpm/node_modules:/home/runner/work/zephyr3d/zephyr3d/common/temp/node_modules:/home/runner/work/zephyr3d/zephyr3d/common/node_modules:/home/runner/work/zephyr3d/zephyr3d/node_modules:/home/runner/work/zephyr3d/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/zephyr3d/zephyr3d/examples/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../common/temp/node_modules/.pnpm/shx@0.3.4/node_modules/shx/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../common/temp/node_modules/.pnpm/shx@0.3.4/node_modules/shx/lib/cli.js" "$@"
fi
